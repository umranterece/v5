{
  "name": "agora-video-conference-v5",
  "version": "5.0.0",
  "description": "Modern, özelleştirilebilir ve güçlü video konferans uygulaması. Agora SDK kullanarak gerçek zamanlı iletişim, ekran paylaşımı ve gelişmiş loglama özellikleri sunar.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "vercel-build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "agora-rtc-sdk-ng": "^4.20.0",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.5",
    "vite": "^5.1.4",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "agora",
    "video-conference",
    "video-call",
    "webrtc",
    "vue3",
    "vue",
    "real-time",
    "communication",
    "screen-sharing",
    "recording",
    "logging"
  ],
  "author": "Umran Terece",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/umranterece/v5.git"
  },
  "bugs": {
    "url": "https://github.com/umranterece/v5/issues"
  },
  "homepage": "https://github.com/umranterece/v5#readme"
}
